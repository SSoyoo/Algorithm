package test;

import java.util.Scanner;

public class A_백준_14681 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int x = sc.nextInt();
		int y = sc.nextInt();
		
		//1사분면은 둘다 양수
		//2사분면은 x만음수
		//3사분면은 둘다음수
		//4사분면은 y만음수
		
		if(x>0 && y<0) {
			System.out.println("1");
		}else if(x<0 && y>0) {
			System.out.println("2");
		}else if(x<0 && y<00) {
			System.out.println("3");
		}else System.out.println("4");
		
	}

}
