package test;

import java.util.Scanner;

public class A_백준_3003 {

	public static void main(String[] args) {
/*
 * 킹, 퀸 룩 비숍 나이트 폰의 개수가 주어진다
 * 출력 첫째 줄에 주어진 순서대로 몇개의 피스를 더하가나 빼면 되는지 출력
 */
		
		int[] chess = new int[6];
		Scanner sc = new Scanner(System.in);
		
		for(int i =0 ; i<chess.length ; i++) {
			System.out.println("말의 개수를 입력하세요");
			chess[i] = sc.nextInt();
		}
		
		System.out.println();
	}

}
